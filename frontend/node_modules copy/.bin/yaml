#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Website/msc/MSC_Last_Version/MSC_REBUILD/node_modules/yaml/node_modules:/mnt/d/Website/msc/MSC_Last_Version/MSC_REBUILD/node_modules:/mnt/d/Website/msc/MSC_Last_Version/node_modules:/mnt/d/Website/msc/node_modules:/mnt/d/Website/node_modules:/mnt/d/node_modules:/mnt/d/Website/msc/MSC_Last_Version/MSC_REBUILD/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Website/msc/MSC_Last_Version/MSC_REBUILD/node_modules/yaml/node_modules:/mnt/d/Website/msc/MSC_Last_Version/MSC_REBUILD/node_modules:/mnt/d/Website/msc/MSC_Last_Version/node_modules:/mnt/d/Website/msc/node_modules:/mnt/d/Website/node_modules:/mnt/d/node_modules:/mnt/d/Website/msc/MSC_Last_Version/MSC_REBUILD/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../yaml/bin.mjs" "$@"
else
  exec node  "$basedir/../yaml/bin.mjs" "$@"
fi
